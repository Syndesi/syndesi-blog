{
  "presets": [
    "env", "react", "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-dynamic-import",
    ["import-inspector", {
      "serverSideRequirePath": true
    }],
    ["prismjs", {
      "languages": ["arduino", "bash", "css", "java", "javascript", "json", "jsx", "markdown", "markup", "php", "python", "scss", "sql", "yaml"],
      "plugins": ["line-numbers"],
      "css": true
    }]
  ]
}